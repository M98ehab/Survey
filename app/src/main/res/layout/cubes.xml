        android:id="@+id/dy7"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/dy6"
        android:layout_alignStart="@+id/dy6" />

    <Button
        android:id="@+id/calc"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:background="@drawable/sev"
        android:text="calc"
        android:textColor="@android:color/white"
        android:visibility="invisible"
        tools:ignore="HardcodedText"
        android:layout_above="@+id/dy5"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/enter"
        android:layout_width="70dp"
        android:layout_height="33dp"
        android:layout_alignBottom="@+id/sides"
        android:layout_alignStart="@+id/calc"
        android:background="@drawable/sev"
        android:text="Enter"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/error"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:background="@drawable/sev"
        android:visibility="invisible"
        android:text="c.error"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="17dp" />

    <TextView
        android:id="@+id/closing"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:textSize="22dp"
        tools:ignore="HardcodedText,SpUsage"
        android:layout_alignBaseline="@+id/error"
        android:layout_alignBottom="@+id/error"
        android:layout_alignEnd="@+id/dx" />

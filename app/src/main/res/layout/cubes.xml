        android:visibility="invisible"
        tools:ignore="HardcodedText,LabelFor,SpUsage"
        android:layout_below="@+id/b6"
        android:layout_alignStart="@+id/b6" />

    <TextView
        android:id="@+id/dx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sides"
        android:layout_marginStart="21dp"
        android:layout_toEndOf="@+id/bearings"
        android:text="dx"
        android:textColor="@android:color/white"
        android:textSize="21dp"
        android:visibility="invisible"
        tools:ignore="HardcodedText,SpUsage" />

    <TextView
        android:id="@+id/dx1"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText,SmallSp"
        android:layout_below="@+id/dx"
        android:layout_alignStart="@+id/dx" />

    <TextView
        android:id="@+id/dx2"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/dx1"
        android:layout_below="@+id/dx1"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/dx3"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/dx1"
        android:layout_below="@+id/dx2"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/dx4"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/dx1"
        android:layout_below="@+id/dx3"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/dx5"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/dx1"
        android:layout_below="@+id/dx4"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/dx6"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/dx1"
        android:layout_below="@+id/dx5"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/dx7"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:layout_alignStart="@+id/dx1"
        android:layout_below="@+id/dx6"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/dy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dy"
        android:textColor="@android:color/white"
        android:textSize="21dp"
        android:visibility="invisible"
        tools:ignore="HardcodedText,SpUsage"
        android:layout_below="@+id/sides"
        android:layout_alignEnd="@+id/sides"
        android:layout_marginEnd="18dp" />

    <TextView
        android:id="@+id/dy1"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/dy"
        android:layout_alignStart="@+id/dy" />

    <TextView
        android:id="@+id/dy2"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText,UnknownId"
        android:layout_below="@+id/dy1"
        android:layout_alignStart="@+id/dy1" />

    <TextView
        android:id="@+id/dy3"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/dy2"
        android:layout_alignStart="@+id/dy2" />

    <TextView
        android:id="@+id/dy4"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/dy3"
        android:layout_alignStart="@+id/dy3" />

    <TextView
        android:id="@+id/dy5"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/dy4"
        android:layout_alignStart="@+id/dy4" />

    <TextView
        android:id="@+id/dy6"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:textColor="@android:color/white"
        tools:ignore="HardcodedText"
        android:layout_below="@+id/dy5"
